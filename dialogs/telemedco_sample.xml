                            <goto ref="folder_q_11"/>
                        </input>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Live Content" id="folder_q_11">
                <output>
                    <prompt selectionType="RANDOM">
                        <item>Where does it hurt?</item>
                    </prompt>
                    <getUserInput>
                        <input>
                            <grammar>
                                <item>$ (Hurt)={Hurt1} * (Hurt)={Hurt2} * (Hurt)={Hurt3}</item>
                                <item>$ (Hurt)={Hurt1} * (Hurt)={Hurt2} (Hurt)={Hurt3}</item>
                                <item>$ (Hurt)={Hurt1} (Hurt)={Hurt2} * (Hurt)={Hurt3}</item>
                                <item>$ (Hurt)={Hurt1} (Hurt)={Hurt2} (Hurt)={Hurt3}</item>
                                <item>$ (Hurt)={Hurt1} * (Hurt)={Hurt2}</item>
                                <item>$ (Hurt)={Hurt1} (Hurt)={Hurt2}</item>
                                <item>$ (Hurt)={Hurt1}</item>
                            </grammar>
                            <action varName="Hurt1" operator="SET_TO">{Hurt1.value:main}</action>
                            <action varName="Hurt2" operator="SET_TO">{Hurt2.value:main}</action>
                            <action varName="Hurt3" operator="SET_TO">{Hurt3.value:main}</action>
                            <goto ref="folder_q_12"/>
                        </input>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Live Content" id="folder_q_12">
                <output>
                    <prompt selectionType="RANDOM">
                        <item>How long has it been hurting?</item>
                    </prompt>
                    <getUserInput>
                        <input>
                            <grammar>
                                <item>$ (Hurt_Duration)={Hurt_Duration_Time}</item>
                            </grammar>
                            <action varName="Hurt_Duration_Time" operator="SET_TO">{Hurt_Duration_Time.value:main}</action>
                            <goto ref="folder_bye"/>
                        </input>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Live Content" id="folder_bye">
                <output>
                    <prompt selectionType="RANDOM">
                        <item>Thank you. Your Risk Factor is {Risk}</item>
                    </prompt>
                    <goto ref="reset_conversation"/>
                </output>
            </folder>
            <folder label="Live Content" id="reset_conversation">
                <getUserInput>
                    <input>
                        <grammar>
                            <item>*</item>
                        </grammar>
                        <action varName="Status" operator="SET_TO_BLANK"/>
                        <action varName="Risk" operator="SET_TO">0</action>
                        <action varName="Hurt1" operator="SET_TO_BLANK"/>
                        <action varName="Hurt2" operator="SET_TO_BLANK"/>
                        <action varName="Hurt3" operator="SET_TO_BLANK"/>
                        <action varName="Hurt_Duration_Time" operator="SET_TO_BLANK"/>
                    </input>
                </getUserInput>
            </folder>
            <folder label="Storage"/>
        </folder>
        <folder label="Global"/>
        <folder label="Concepts"/>
    </flow>
    <entities>
