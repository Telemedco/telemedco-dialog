<?xml version="1.0" encoding="UTF-8"?>
<dialog xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <flow>
        <folder label="Main">
            <output>
                <prompt selectionType="RANDOM">
                    <item>Are you experiencing pain in your chest?</item>
                </prompt>
                <goto ref="getUserInput_1"/>
            </output>
            <output>
                <prompt selectionType="SEQUENTIAL">
                    <item>Are you experiencing pain in your chest?</item>
                </prompt>
                <getUserInput id="getUserInput_1">
                    <search ref="folder_q_1"/>
                    <default>
                        <output>
                            <prompt selectionType="RANDOM">
                                <item>I'm sorry I did not understand your question.</item>
                                <item>I did not quite get that.</item>
                                <item>I didn't grasp what you said.</item>
                                <item>I'm not following you at the moment.</item>
                            </prompt>
                        </output>
                    </default>
                </getUserInput>
            </output>
        </folder>
        <folder label="Library">
            <folder label="Live Content" id="folder_q_1">
                <input>
                    <grammar>
                        <item>yes</item>
                    </grammar>
                    <action varName="Status" operator="SET_TO">yes</action>
                    <action varName="Risk"
                        operator="INCREMENT_BY">1</action>
                    <goto ref="folder_q_2"/>
                </input>
                <input>
                    <grammar>
                        <item>no</item>
                    </grammar>
                    <action varName="Status" operator="SET_TO">no</action>
                    <goto ref="folder_q_2"/>
                </input>
            </folder>
            <folder label="Live Content" id="folder_q_2">
                <output>
                    <prompt selectionType="RANDOM">
                        <item>Do you use tobacco?</item>
                    </prompt>
                    <getUserInput>
