                            <goto ref="folder_q_3"/>
                        </input>
                        <input>
                            <grammar>
                                <item>no</item>
                            </grammar>
                            <action varName="Status" operator="SET_TO">no</action>
                             <goto ref="folder_q_3"/>
                        </input>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Live Content" id="folder_q_3">
