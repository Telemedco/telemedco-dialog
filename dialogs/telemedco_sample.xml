                            <action varName="Risk"
                                operator="INCREMENT_BY">1</action>
                            <goto ref="folder_q_4"/>
                        </input>
                        <input>
                            <grammar>
                                <item>no</item>
                            </grammar>
                            <action varName="Status" operator="SET_TO">no</action>
                            <goto ref="folder_q_4"/>
                        </input>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Live Content" id="folder_q_4">
