            <folder label="Live Content" id="folder_q_7">
                <output>
                    <prompt selectionType="RANDOM">
                        <item>Are you a male over the age of 45 or a female over the age of 55?</item>
                    </prompt>
                    <getUserInput>
                        <input>
                            <grammar>
                                <item>yes</item>
                            </grammar>
                            <action varName="Status" operator="SET_TO">yes</action>
                            <action varName="Risk"
